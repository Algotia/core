{
    "presets": [
      ["@babel/preset-env", { 
        "modules": false,
        "targets": {
          "node": "current"
        }
        }],
      ["@babel/preset-react"]
    ],
    "plugins": [
      "@babel/transform-flow-strip-types",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  }
  